# Configuración de ejemplo para usar con Claude
# Guarda esto en un archivo .env o añádelo a tu ~/.bashrc

# Para usar Claude como proveedor por defecto
export AI_PROVIDER="claude"
export CLAUDE_API_KEY="tu-claude-api-key-aquí"

# Para usar Gemini (configuración existente)
export GEMINI_API_KEY="tu-gemini-api-key-aquí"

# También puedes usar Google API Key si prefieres
export GOOGLE_API_KEY="tu-google-api-key-aquí"
